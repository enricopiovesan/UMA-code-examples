{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "telemetry.validation.v1",
  "type": "object",
  "required": [
    "source",
    "event",
    "status"
  ],
  "properties": {
    "source": {
      "type": "string"
    },
    "event": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "passed",
        "failed"
      ]
    },
    "reason": {
      "type": "string"
    }
  },
  "additionalProperties": false
}